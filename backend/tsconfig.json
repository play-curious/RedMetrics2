{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2020",
    "lib": ["es2020"],
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}