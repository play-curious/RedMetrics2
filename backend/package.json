{
  "name": "red-metrics",
  "version": "2.0.0",
  "description": "Open game analytics",
  "scripts": {
    "build": "tsc -p tsconfig.app.json",
    "serve": "npm run build && node dist/index.js",
    "build-migrations": "tsc -p tsconfig.migrations.json",
    "migrate": "npm run build-migrations && npx knex migrate:latest",
    "migrate-down": "npm run build-migrations && npx knex migrate:down",
    "seed": "npx knex seed:run",
    "debug": "set DEBUG=express:* & npm run start",
    "start": "npm run migrate && npm run serve",
    "watch": "npm run migrate && tsc-watch --onSuccess \"node dist/index.js\"",
    "format": "prettier --write src tests",
    "test": "tsc && jest",
    "clean": "rm -rf dist",
    "types": "npm up rm2-typings -f",
    "fixtures": "node dist/fixtures/setup",
    "fixtures-down": "node dist/fixtures/teardown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/play-curious/RedMetrics2.git"
  },
  "keywords": [],
  "author": "play-curious",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/play-curious/RedMetrics2/issues"
  },
  "homepage": "https://github.com/play-curious/RedMetrics2#readme",
  "dependencies": {
    "@jest/globals": "^27.4.4",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^5.0.0-alpha.8",
    "faker": "^5.5.3",
    "knex": "^0.95.14",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "rm2-typings": "git+https://github.com/play-curious/RedMetrics2-typing.git",
    "tsc-watch": "^4.5.0",
    "underscore": "^1.13.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/underscore": "^1.11.4",
    "@types/uuid": "8.3.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6",
    "typescript": "^4.5.4"
  }
}
