{
  "name": "red-metrics",
  "version": "2.0.0",
  "description": "Open game analytics",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.app.json",
    "serve": "yarn build && node .",
    "build-migrations": "tsc -p tsconfig.migrations.json",
    "migrate": "yarn build-migrations && yarn knex migrate:latest",
    "start": "yarn migrate && yarn serve",
    "format": "yarn prettier",
    "watch": "tsc-watch --onSuccess \"node .\"",
    "prettier": "prettier --write src tests",
    "test": "tsc && jest",
    "docker": "docker-compose up --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/play-curious/RedMetrics2.git"
  },
  "keywords": [],
  "author": "play-curious",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/play-curious/RedMetrics2/issues"
  },
  "homepage": "https://github.com/play-curious/RedMetrics2#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "dayjs": "^1.9.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-graphql": "^0.11.0",
    "graphql": "^15.4.0",
    "knex": "^0.21.15",
    "pg": "^8.5.0",
    "tsc-watch": "^4.2.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.20",
    "@types/pg": "^7.14.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "8.3.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "superagent": "^6.1.0",
    "supertest": "^6.0.1",
    "typescript": "^4.1.3",
    "yarn": "^1.22.10"
  }
}
